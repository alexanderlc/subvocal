{
    "id": "akka-worker",
    "kind": "Pod",
    "apiVersion": "v1beta1",
    "desiredState": {
        "manifest": {
            "version": "v1beta1",
            "id": "akka-worker",
            "containers": [{
                "name": "akka-worker",
                "image": "gcr.io/subvocal_01/subvocal-web:latest",
                "cpu": 100,
                "ports": [{ "name": "akka-worker", "containerPort": 2551 }],
                "env": [{"name": "SPRING_PROFILE", "value": "worker"}]
            }]
        }
    },
    "labels": {
        "name": "akka", "role": "worker"
    }
}